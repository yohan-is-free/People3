<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.people3.model.mapper.TestMapper">
    
    <select id="getLists" resultType="com.people3.model.vo.Board">
		select * from board order by boardNo desc
	</select>
    
    
    <select id = "testDB" resultType = "java.lang.String">
    	select now() from dual;
    </select>
    
    <insert id="boardInsert" parameterType="com.people3.model.vo.Board">
	INSERT INTO board(id,title,content,date)
	VALUES(#{id}, #{title}, #{content}, now())
    </insert>
</mapper>